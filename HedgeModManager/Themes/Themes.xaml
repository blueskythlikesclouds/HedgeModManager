<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HedgeModManager.Themes">

    <local:ThemeList x:Key="Themes">
        <local:ThemeEntry FileName="DarkTheme" Name="Dark"/>
        <local:ThemeEntry FileName="DarkerTheme" Name="Darker"/>
        <local:ThemeEntry FileName="OLEDTheme" Name="OLED"/>
        <local:ThemeEntry FileName="LightTheme" Name="Light"/>
        <local:ThemeEntry FileName="W11DarkTheme" Name="Windows 11 Dark"/>
        <local:ThemeEntry FileName="W11LightTheme" Name="Windows 11 Light"/>
    </local:ThemeList>

</ResourceDictionary>